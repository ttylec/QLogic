  RowBox[{
   RowBox[{"compatibilityCond", "[", 
    RowBox[{"p_", ",", "q_", ",", "a_", ",", "b_", ",", "c_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"isQEqual", "[", 
     RowBox[{"p", ",", 
      RowBox[{"a", "\[CirclePlus]", "c"}]}], "]"}], "&&", 
    RowBox[{"isQEqual", "[", 
     RowBox[{"q", ",", 
      RowBox[{"b", "\[CirclePlus]", "c"}]}], "]"}]}]}], 
