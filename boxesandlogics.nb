Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"isQEqual", "[", 
    RowBox[{"q_", ",", "r_"}], "]"}], ":=", 
   RowBox[{"With", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"gs", "=", 
       RowBox[{"generalState", "[", "]"}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
