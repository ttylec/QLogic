 RowBox[{
  RowBox[{
   RowBox[{"pr", "[", 
    RowBox[{
     RowBox[{"question", "[", "0", "]"}], ",", "s_"}], "]"}], ":=", "0"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"pr", "[", 
    RowBox[{"q_question", ",", "s_state"}], "]"}], ":=", 
