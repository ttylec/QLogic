      "\[IndentingNewLine]", 
      RowBox[{"Plus", "@@", 
       RowBox[{"(", 
        RowBox[{"Thread", "[", 
         RowBox[{"Times", "[", 
          RowBox[{"rv", ",", "cc"}], "]"}], "]"}], ")"}]}]}], "]"}], "+", 
    "1"}]}], ";"}], "\[IndentingNewLine]", 
