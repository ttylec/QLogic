Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"state2matrix", "[", "s_state", "]"}], ":=", 
   RowBox[{
    RowBox[{"List", "[", 
     RowBox[{"Sequence", "@@", "s"}], "]"}], "[", 
    RowBox[{"[", "1", "]"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"calculateIndex", "[", 
    RowBox[{"v_", ",", "cc_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"With", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"rv", " ", "=", " ", 
        RowBox[{
