Cell["Auxilliary function for calculating probabilities", "Text",
 CellChangeTimes->{{3.5599900619383383`*^9, 3.559990072579523*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"state2matrix", "[", "s_state", "]"}], ":=", 
   RowBox[{
    RowBox[{"List", "[", 
     RowBox[{"Sequence", "@@", "s"}], "]"}], "[", 
    RowBox[{"[", "1", "]"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"calculateIndex", "[", 
    RowBox[{"v_", ",", "cc_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"With", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"rv", " ", "=", " ", 
        RowBox[{
         RowBox[{
          RowBox[{"#", "-", "1"}], "&"}], "/@", "v"}]}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"Plus", "@@", 
       RowBox[{"(", 
        RowBox[{"Thread", "[", 
         RowBox[{"Times", "[", 
          RowBox[{"rv", ",", "cc"}], "]"}], "]"}], ")"}]}]}], "]"}], "+", 
    "1"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"observableIndex", "[", "q_question", "]"}], ":=", 
   RowBox[{"calculateIndex", "[", 
    RowBox[{
