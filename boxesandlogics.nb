  RowBox[{"Or", "@@", 
   RowBox[{"Map", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"isQEqual", "[", 
       RowBox[{"a", ",", " ", "#"}], "]"}], "&"}], ",", " ", "l"}], 
