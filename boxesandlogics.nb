 RowBox[{
  RowBox[{
   RowBox[{"compatibilityTest", "[", 
    RowBox[{"p_", ",", "q_"}], "]"}], ":=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"Select", "[", 
        RowBox[{"mor1q", ",", " ", 
         RowBox[{
          RowBox[{"compatibilityCond", "[", 
           RowBox[{"p", ",", "q", ",", 
            RowBox[{"Sequence", "@@", "#"}]}], "]"}], "&"}]}], "]"}], 
       "\[Equal]", 
       RowBox[{"{", "}"}]}], " ", "&&", " ", 
      RowBox[{"Not", "[", 
       RowBox[{"isLess", "[", 
        RowBox[{"p", ",", 
         RowBox[{"orthocmpl", "[", "q", "]"}]}], "]"}], "]"}]}], ",", " ", 
     "False", ",", " ", "True"}], "]"}]}], 
