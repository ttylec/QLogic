Cell["Check if two question are equal", "Text",
 CellChangeTimes->{{3.5665844663010473`*^9, 3.566584475798304*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"isQEqual", "[", 
    RowBox[{"q_", ",", "r_"}], "]"}], ":=", 
   RowBox[{"With", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"gs", "=", 
       RowBox[{"generalState", "[", "]"}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"pr", "[", 
          RowBox[{"q", ",", "gs"}], "]"}], "\[Equal]", 
         RowBox[{"pr", "[", 
          RowBox[{"r", ",", "gs"}], "]"}]}], "===", "False"}], ",", " ", 
       "False", ",", 
       RowBox[{
        RowBox[{"MatrixRank", "[", 
         RowBox[{
          RowBox[{"CoefficientArrays", "[", 
           RowBox[{
            RowBox[{"Join", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{
                RowBox[{"pr", "[", 
                 RowBox[{"q", ",", "gs"}], "]"}], "\[Equal]", 
                RowBox[{"pr", "[", 
                 RowBox[{"r", ",", "gs"}], "]"}]}], "}"}], ",", 
              "restrictedEqs"}], "]"}], ",", 
            RowBox[{"Flatten", "[", 
             RowBox[{"Sequence", "@@", "gs"}], "]"}]}], "]"}], "[", 
          RowBox[{"[", "2", "]"}], "]"}], "]"}], "\[Equal]", 
        "rankOfRestrictedCoeffArray"}]}], "]"}]}], "]"}]}], ";"}]], "Input",
