      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"pr", "[", 
          RowBox[{"q", ",", "gs"}], "]"}], "\[Equal]", 
         RowBox[{"pr", "[", 
          RowBox[{"r", ",", "gs"}], "]"}]}], "===", "False"}], ",", " ", 
       "False", ",", 
       RowBox[{
        RowBox[{"MatrixRank", "[", 
         RowBox[{
          RowBox[{"CoefficientArrays", "[", 
           RowBox[{
            RowBox[{"Join", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{
                RowBox[{"pr", "[", 
                 RowBox[{"q", ",", "gs"}], "]"}], "\[Equal]", 
                RowBox[{"pr", "[", 
                 RowBox[{"r", ",", "gs"}], "]"}]}], "}"}], ",", 
              "restrictedEqs"}], "]"}], ",", 
            RowBox[{"Flatten", "[", 
             RowBox[{"Sequence", "@@", "gs"}], "]"}]}], "]"}], "[", 
          RowBox[{"[", "2", "]"}], "]"}], "]"}], "\[Equal]", 
        "rankOfRestrictedCoeffArray"}]}], "]"}]}], "]"}]}], ";"}]], "Input",
